{
  "name": "recipe-book",
  "version": "0.1.0",
  "private": true,
  "description": "A full-stack web application for managing recipes and ingredients.",
  "homepage": "https://github.com/blwatkins/recipe-book#readme",
  "bugs": {
    "url": "https://github.com/blwatkins/recipe-book/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blwatkins/recipe-book.git"
  },
  "license": "MIT",
  "author": "brittni watkins",
  "type": "module",
  "main": "src/app.mjs",
  "scripts": {
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "build:watch": "webpack --mode=development --watch",
    "start": "dotenvx run -f .env.prod -- node src/app.mjs",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "dotenvx run -f .env -- nodemon src/app.mjs",
    "dev:prod-config": "npm run build; dotenvx run -f .env.prod -- nodemon src/app.mjs"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.51.2",
    "cors": "^2.8.5",
    "ejs": "^3.1.10",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "mysql2": "^3.16.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.11",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-middleware": "^7.4.5"
  }
}
